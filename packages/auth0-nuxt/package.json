{
  "name": "@auth0/auth0-nuxt",
  "version": "1.0.0",
  "description": "Auth0 Authentication SDK for Nuxt Applications on JavaScript runtimes",
  "author": "Auth0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nuxt-module-build build --stub",
    "postbuild": "nuxt-module-build prepare",
    "build:watch": "tsup --watch",
    "clean": "rm -rf ./dist",
    "lint": "eslint \"./**/*.ts*\"",
    "test": "vitest run",
    "test:ci": "vitest --watch false --coverage"
  },
  "module": "./dist/module.mjs",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@nuxt/devtools": "^2.4.1",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.4",
    "@nuxt/test-utils": "^3.19.1",
    "@vitest/coverage-v8": "^3.0.6",
    "eslint": "^9.20.1",
    "nuxt": "^3.17.4",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@auth0/auth0-server-js": "^1.0.3",
    "@nuxt/kit": "^3.17.4"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
      "access": "public"
  },
  "repository": {
      "type": "git",
      "url": "git://github.com/auth0/auth0-nuxt.git"
  },
  "bugs": {
      "url": "https://github.com/auth0/auth0-nuxt/issues"
  },
  "homepage": "https://github.com/auth0/auth0-nuxt#readme",
  "keywords": [
      "auth0"
  ]
}
