{
  "name": "@auth0/auth0-nuxt",
  "version": "1.0.0",
  "description": "Auth0 Authentication SDK for Nuxt Applications on JavaScript runtimes",
  "author": "Auth0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf ./dist",
    "lint": "eslint \"./**/*.ts*\"",
    "test": "vitest run",
    "test:ci": "vitest --watch false --coverage"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@vitest/coverage-v8": "^3.0.6",
    "eslint": "^9.20.1",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@auth0/auth0-server-js": "^1.0.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
      "access": "public"
  },
  "repository": {
      "type": "git",
      "url": "git://github.com/auth0/auth0-nuxt.git"
  },
  "bugs": {
      "url": "https://github.com/auth0/auth0-nuxt/issues"
  },
  "homepage": "https://github.com/auth0/auth0-nuxt#readme",
  "keywords": [
      "auth0"
  ]
}
