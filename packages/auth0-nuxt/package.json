{
  "name": "@auth0/auth0-nuxt",
  "version": "1.0.0-beta.0",
  "description": "Auth0 Authentication SDK for Nuxt Applications on JavaScript runtimes",
  "author": "Auth0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nuxt-module-build build --stub",
    "postbuild": "nuxt-module-build prepare",
    "build:watch": "tsup --watch",
    "clean": "rm -rf ./dist",
    "lint": "eslint \"./**/*.ts*\"",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run ./src/**",
    "test:e2e": "vitest run ./test/**",
    "test:ci": "vitest --watch false --coverage",
    "pretest": "npx playwright install",
    "pretest:ci": "npx playwright install"
  },
  "module": "./dist/module.mjs",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "typeRoots": [
    "./node_modules/@types",
    "./src/types"
  ],
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@nuxt/devtools": "^2.4.1",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/kit": "^4.1.3",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.19.2",
    "@vitest/coverage-v8": "^3.0.6",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.35.0",
    "happy-dom": "^18.0.1",
    "nock": "^14.0.5",
    "nuxt": "^4.1.3",
    "playwright": "^1.53.2",
    "playwright-core": "^1.53.2",
    "tsup": "^8.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@auth0/auth0-server-js": "^1.0.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/auth0/auth0-nuxt.git"
  },
  "bugs": {
    "url": "https://github.com/auth0/auth0-nuxt/issues"
  },
  "homepage": "https://github.com/auth0/auth0-nuxt#readme",
  "keywords": [
    "auth0"
  ]
}
